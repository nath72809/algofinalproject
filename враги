i = 0
class Enemy(GameSprite):
   def update(self):
       if self.rect.x >= 50 and self.rect.x <= 450 and i == 0:
           self.rect.x += 10
       if self.rect.x >= 450:
           self.rect.y += 25
           i += 1
       if self.rect.x <= 50:
           self.rect.y += 25
           i -= 1
        if self.rect.x >= 50 and self.rect.x <= 450 and i == 1:
           self.rect.x -= 10
